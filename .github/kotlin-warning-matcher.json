{
  // Example:
  // w: /path/to/file/KotlinFile.kt: (14, 5): Parameter 'foo' is never used
  "problemMatcher": [
    {
      "owner": "kotlin-warning",
      "pattern": [
        {
          "regexp": "^w:\\s+(\\S+):\\s+\\((\\d+),\\s+(\\d+)\\):\\s+(.+)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": 4
        }
      ],
      "severity": "warning"
    }
  ]
}
